        f"{standingDunk:2d} Standing Dunk\n"
        f"{drivingDunk:2d} Driving Dunk\n"
        f"{postHook:2d} Post Hook\n"
        f"{postFade:2d} Post Fade\n"
        f"{postControl:2d} Post Control\n"
        f"{drawFoul:2d} Draw Foul\n"
        f"{hands:2d} Hands\n"
        f"\n"
        f"{playmaking:2d} Playmaking\n"
        f"-------------------------------------\n"
        f"{passAccuracy:2d} Pass Accuracy\n"
        f"{ballHandle:2d} Ball Handle\n"
        f"{speedWithBall:2d} Speed with Ball\n"
        f"{passIQ:2d} Pass IQ\n"
        f"{passVision:2d} Pass Vision\n"
        f"\n"
        f"{defending:2d} Defending\n"
        f"-------------------------------------\n"
        f"{interiorDefense:2d} Interior Defense\n"
        f"{perimeterDefense:2d} Perimeter Defense\n"
        f"{steal:2d} Steal\n"
        f"{block:2d} Block\n"
        f"{lateralQuickness:2d} Lateral Quickness\n"
        f"{helpDefenseIQ:2d} Help Defense IQ\n"
        f"{passPerception:2d} Pass Perception\n"
        f"{defensiveConsistency:2d} Defensive Consistency\n"
        f"\n"
        f"{rebounding:2d} Rebounding\n"
        f"-------------------------------------\n"
        f"{offensiveRebound:2d} Offensive Rebound\n"
        f"{defensiveRebound:2d} Defensive Rebound\n"
        f"\n"
        f"{intangibles:2d} Intangibles\n"
        f"{potential:2d} Potential\n"
        f"{height} cm ({calcCMtoInch(height)} ft)\n"
        f"{weight} KG ({round(weight * 2.20462)} lbs)\n"
        f"\n"
    )
    badges = [
        f"{'Acrobat: Bronze' if layup >= 75 and layup < 80 else ''}",
        f"{'Acrobat: Silver' if layup >= 80 and layup < 85 else ''}",
        f"{'Acrobat: Gold' if layup >= 85 and layup < 90 else ''}",
        f"{'Acrobat: Hall of Fame' if layup >= 90 else ''}",
    
        f"{'Aerial Wizard: Bronze' if max(drivingDunk, standingDunk) >= 75 and max(drivingDunk, standingDunk) < 80 else ''}",
        f"{'Aerial Wizard: Silver' if max(drivingDunk, standingDunk) >= 80 and max(drivingDunk, standingDunk) < 85 else ''}",
        f"{'Aerial Wizard: Gold' if max(drivingDunk, standingDunk) >= 85 and max(drivingDunk, standingDunk) < 90 else ''}",
        f"{'Aerial Wizard: Hall of Fame' if max(drivingDunk, standingDunk) >= 90 else ''}",
    
        f"{'Backdown Punisher: Bronze' if postControl >= 75 and postControl < 80 and strength >= 75 else ''}",
        f"{'Backdown Punisher: Silver' if postControl >= 80 and postControl < 85 and strength >= 80 else ''}",
        f"{'Backdown Punisher: Gold' if postControl >= 85 and postControl < 90 and strength >= 85 else ''}",

import math
import random

MIN = 40
TEMPMIN = 50
MAX = 59

def calcCMtoInch(heightCM):
    feet = math.floor(heightCM / 30.48)
    inches = round((heightCM % 30.48) / 2.54)
    if inches == 12:
        feet += 1
        inches = 0
    newHeight = "{}'{}\"".format(feet, inches)
    return newHeight

def generatePlayer(POS, OSP, ISP, DP, AP, PP, RP):
    PG = 0 
    SG = 0
    SF = 0 
    PF = 0
    C = 0
    if POS == "PG":
        PG = 10
    if POS == "SG":
        SG = 10
    if POS == "SF":
        SF = 10
    if POS == "PF":
        PF = 10
    if POS == "C":
        C = 10
    #Outside Scoring
    baseValue = 1.2
    if OSP:
        baseValue = 1.5
    closeShot = math.ceil(random.randint(MIN, MAX) * baseValue) + (SF + PF + C)
    midRangeShot = math.ceil(random.randint(MIN, MAX) * baseValue) + (PG + SG + SF + PF + C)
    threePointShot = math.ceil(random.randint(MIN, MAX) * baseValue) + (PG + SG + SF)
    freeThrow = math.ceil(random.randint(MIN, MAX) * baseValue) + (PG + SG + SF)
    shotIQ = math.ceil(random.randint(TEMPMIN, MAX) * baseValue) + (PG + SG + SF + PF + C)
    offensiveConsistency = math.ceil(random.randint(TEMPMIN, MAX) * baseValue) + (SG + SF + PF)
    outsideScoring = math.floor((closeShot 
                                + midRangeShot 
                                + threePointShot 
                                + freeThrow 
                                + shotIQ
                                + offensiveConsistency) / 6)
    #Inside Scoring
    baseValue = 1.2
    if ISP:
        baseValue = 1.5
    layup = math.ceil(random.randint(MIN, MAX) * baseValue) + (PG + SG + SF)
    standingDunk = math.ceil(random.randint(MIN, MAX) * baseValue) + (SF + PF + C)
    drivingDunk = math.ceil(random.randint(MIN, MAX) * baseValue) + (SF + PF)
    postHook = math.ceil(random.randint(MIN, MAX) * baseValue) + (PF + C)
    postFade = math.ceil(random.randint(MIN, MAX) * baseValue) + (PG + SG + SF)
    postControl = math.ceil(random.randint(MIN, MAX) * baseValue) + (PF + C)
    drawFoul = math.ceil(random.randint(MIN, MAX) * baseValue) + (PG + SG + SF + PF + C)
    hands = math.ceil(random.randint(TEMPMIN, MAX) * baseValue) + (PG + SG + SF + PF + C)
    insideScoring = math.floor((layup
                                + standingDunk
                                + drivingDunk
                                + postHook
                                + postFade
                                + postControl
                                + drawFoul
                                + hands) / 8)
    #Defending
    baseValue = 1.2
    if DP:
        baseValue = 1.5
    interiorDefense = math.ceil(random.randint(MIN, MAX) * baseValue) + (PF + C)
    perimeterDefense = math.ceil(random.randint(MIN, MAX) * baseValue) + (PG + SG + SF)
    steal = math.ceil(random.randint(MIN, MAX) * baseValue) + (PG + SG + SF)
    block = math.ceil(random.randint(MIN, MAX) * baseValue) + (PF + C)
    lateralQuickness = math.ceil(random.randint(MIN, MAX) * baseValue) + (PG + SG + SF)
    helpDefenseIQ = math.ceil(random.randint(MIN, MAX) * baseValue) + (PG + SG + SF + PF + C)
    passPerception = math.ceil(random.randint(MIN, MAX) * baseValue) + (PG + SG + SF)
    defensiveConsistency = math.ceil(random.randint(TEMPMIN, MAX) * baseValue) + (PG + SG + SF + PF + C)
    defending = math.floor((interiorDefense
                            + perimeterDefense
                            + steal
                            + block
                            + lateralQuickness
                            + helpDefenseIQ
                            + passPerception
                            + defensiveConsistency) / 8)
    #Athleticism
    baseValue = 1.2
    if AP:
        baseValue = 1.5
    speed = math.ceil(random.randint(MIN, MAX) * baseValue) + (PG + SG + SF)
    acceleration = math.ceil(random.randint(MIN, MAX) * baseValue) + (PG + SG + SF)
    strength = math.ceil(random.randint(MIN, MAX) * baseValue) + (PF + C)
    vertical = math.ceil(random.randint(MIN, MAX) * baseValue) + (SG + SF + PF)
    stamina = math.ceil(random.randint(TEMPMIN, MAX) * baseValue) + (PG + SG + SF + PF + C)
    hustle = math.ceil(random.randint(MIN, MAX) * baseValue) + (PG + SG + SF)
    overallDurability = math.ceil(random.randint(TEMPMIN, MAX) * baseValue) + (PG + SG + SF + PF + C)
    athleticism = math.floor((speed
                            + acceleration
                            + strength
                            + vertical
                            + stamina
                            + hustle
                            + overallDurability) / 7)
    #Playmaking
    baseValue = 1.2
    if PP:
        baseValue = 1.5
    passAccuracy = math.ceil(random.randint(MIN, MAX) * baseValue) + (PG + SF + C)
    ballHandle = math.ceil(random.randint(MIN, MAX) * baseValue) + (PG + SG + SF)
    speedWithBall = math.ceil(random.randint(MIN, MAX) * baseValue) + (PG + SG + SF)
    passIQ= math.ceil(random.randint(MIN, MAX) * baseValue) + (PG + C)
    passVision = math.ceil(random.randint(MIN, MAX) * baseValue) + (PG + C)
    playmaking = math.floor((passAccuracy
                            + ballHandle
                            + speedWithBall
                            + passIQ
                            + passVision) / 5)
    #Rebounding
    baseValue = 1.2
    if RP:
        baseValue = 1.5
    offensiveRebound = math.ceil(random.randint(MIN, MAX) * baseValue) + (SF + PF + C)
    defensiveRebound = math.ceil(random.randint(MIN, MAX) * baseValue) + (PF + C)
    rebounding = math.floor((offensiveRebound
                            + defensiveRebound) / 2)
    #Other
    if POS == "PG":
        height = random.randint(175, 201)
        weight = random.randint(69, 88)
    if POS == "SG":
        height = random.randint(191, 206)
        weight = random.randint(75, 95)
    if POS == "SF":
        height = random.randint(196, 208)
        weight = random.randint(83, 103)
    if POS == "PF":
        height = random.randint(201, 213)
        weight = random.randint(90, 110)
    if POS == "C":
        height = random.randint(208, 221)
        weight = random.randint(96, 120)
    intangibles = random.randint(70, 99)
    potential = random.randint(90, 99)
    
    playerStats = (
        f"{outsideScoring:2d} Outside Scoring\n"
        f"-------------------------------------\n"
        f"{closeShot:2d} Close Shot\n"
        f"{midRangeShot:2d} Mid-Range Shot\n"
        f"{threePointShot:2d} Three-Point Shot\n"
        f"{freeThrow:2d} Free Throw\n"
        f"{shotIQ:2d} Shot IQ\n"
        f"{offensiveConsistency:2d} Offensive Consistency\n"
        f"\n"
        f"{athleticism:2d} Athleticism\n"
        f"-------------------------------------\n"
        f"{speed:2d} Speed\n"
        f"{acceleration:2d} Acceleration\n"
        f"{strength:2d} Strength\n"
        f"{vertical:2d} Vertical\n"
